{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "node-static": "^0.7.10",
    "if-env": "^1.0.4",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "tailwindcss": "^0.5.3",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.1.0",
    "url-join": "^4.0.0",
    "avataaars": "^1.1.0",
    "unstated": "^2.0.3",
    "typeface-karmilla": "github:dmfrancisco/typeface-karmilla",
    "prop-types": "^15.6.1",
    "react-loadable": "^5.4.0",
    "prettier": "^1.12.1",
    "primer-tooltips": "^1.5.3"
  },
  "scripts": {
    "prestart": "prettier --write src/**/*.{js,jsx,json,css}",
    "start": "if-env NODE_ENV=production && npm run prod:start || npm run dev:start",
    "dev:start": "npm-run-all -p build:css dev:start:js",
    "dev:start:js": "react-scripts start",
    "prod:start": "static build --gzip",
    "build": "npm-run-all build:css build:js",
    "build:css": "postcss src/index.tw.css -o src/index.css",
    "build:js": "react-scripts build && gzip -rk build && refresh"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5"
  },
  "license": "MIT"
}